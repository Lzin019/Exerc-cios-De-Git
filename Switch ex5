let estação = 2;
switch (estação) {
    case 1:
        console.log("Sua estação é o verão🥵");
        break;
    case 2:
        console.log("Sua estação é o Outono👌");
        break;
    case 3:
        console.log("Sua estação é o Inverno🍧");
        break;
    case 4:
        console.log("Sua estação é a Primavera🍃");
        break;
    default:
        console.log("Sua estação esta inválida❌")
}
