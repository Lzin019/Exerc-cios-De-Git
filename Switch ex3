let num = 3;
switch (num) {
    case 1:
        console.log(236 + 125);
        break;
    case 2:
        console.log(236 - 125);
        break;
    case 3:
        console.log(236 * 125);
        break;
    case 4:
        console.log(236 / 125);
        break;
    default:
        console.log('Número Inválido')
}
